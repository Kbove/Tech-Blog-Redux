{{#each posts as |post|}}

<div class="row mb-4 project posts">
    <div class="col-md-5 title-contribution">
        <h2 class="blog-title">
            <a href="/post/{{post.id}}">{{post.title}}</a>
        </h2>
        <p class="contribution">Created by {{post.user.name}} on {{format_date post.date_created}}</p>
    </div>
    <div class="col-md-7">
        <p>
            {{post.contents}}
        </p>
    </div>
</div>
{{/each}}